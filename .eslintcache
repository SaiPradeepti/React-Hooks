r\n        return {...todo, complete: !todo.complete}\r\n        }\r\n        return todo;\r\n      })\r\n  }\r\n}\r\n\r\nconst ToDo = () => {\r\n    const [todos, dispatch] = useReducer(reducer, []);\r\n    const [name,setName] = useState('');\r\n\r\n    const handleSubmit = (e) => {\r\n        dispatch({ type: ACTIONS.ADD_TODO, payload: {name: name} });\r\n        setName('');\r\n        e.preventDefault();\r\n    }\r\n  \r\n  return (\r\n    <>\r\n      <form onSubmit={handleSubmit}>\r\n          <input type=\"text\" value={name} onChange={(e)=>setName(e.target.value)}/>\r\n      </form>\r\n      {\r\n          todos.map(todo => {\r\n              return (\r\n                  <div key={todo.id}>\r\n                      <span style={{color: todo.complete ? 'AAA' : '#000'}}>{todo.name}</span>\r\n                      <button onClick={()=>dispatch({type: ACTIONS.TOGGLE_TODO, payload:{id: todo.id}})}>Toggle</button>\r\n                      <button onClick={()=>dispatch({type: ACTIONS.DELETE_TODO, payload:{id: todo.id}})}>Delete</button>\r\n                  </div>\r\n              );\r\n          })\r\n      }\r\n    </>\r\n  );\r\n}\r\n\r\nexport default ToDo;",{"ruleId":"24","replacedBy":"25"},{"ruleId":"26","replacedBy":"27"},{"ruleId":"28","severity":1,"message":"29","line":3,"column":8,"nodeType":"30","messageId":"31","endLine":3,"endColumn":12},{"ruleId":"32","severity":1,"message":"33","line":13,"column":3,"nodeType":"34","messageId":"35","endLine":23,"endColumn":4},"no-native-reassign",["36"],"no-negated-in-lhs",["37"],"no-unused-vars","'ToDo' is defined but never used.","Identifier","unusedVar","default-case","Expected a default case.","SwitchStatement","missingDefaultCase","no-global-assign","no-unsafe-negation"]